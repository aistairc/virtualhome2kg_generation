{
  "name": "kgrc-script-editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_SPARQL_ENDPOINT=http://localhost:8080/sparql next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "check": "ts-node -r tsconfig-paths/register scripts/checkScriptData.ts",
    "convert:objects": "ts-node scripts/convertObjects.ts",
    "convert:actions": "ts-node scripts/convertActions.ts",
    "prebuild": "run-s convert:objects convert:actions",
    "test": "jest"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "testMatch": [
      "/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@next/font": "13.1.2",
    "@types/sparql-http-client": "^2.2.8",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sparql-http-client": "^2.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/rdf-js": "^4.0.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.2",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
